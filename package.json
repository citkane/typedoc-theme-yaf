{
	"name": "typedoc-theme-yaf",
	"version": "0.0.0",
	"description": "Typedoc custom theme for YAF-Scaffold",
	"main": "dist/index",
	"workspaces": [
		"tsconfig"
	],
	"scripts": {
		"docs": "typedoc",
		"build": "tsc --build",
		"build:css": "sass src/assets/scss/style.scss:dist/assets/css/style.css --style compressed",
		"build:hot": "node dist/dev/watcher"
	},
	"author": "Michael Jonker",
	"license": "MIT",
	"devDependencies": {
		"@typedoc-theme-yaf/tsconfig": "*",
		"@types/chokidar": "^2.1.3",
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^18.0.3",
		"browser-sync": "^2.27.10",
		"chokidar": "^3.5.3",
		"fs-extra": "^10.1.0",
		"sass": "^1.53.0",
		"typedoc": "file:../yaf-scaffold/node_modules/typedoc",
		"typescript": "^4.7.4"
	}
}
