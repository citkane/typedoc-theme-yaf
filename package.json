{
	"name": "typedoc-theme-yaf",
	"version": "0.0.0",
	"description": "Typedoc custom theme for YAF-Scaffold",
	"main": "dist/src/index",
	"workspaces": [
		"yaf-scaffold/packages/*",
		"yaf-scaffold/src"
	],
	"scripts": {
		"docs": "typedoc",
		"docs:serve": "node node_modules/.bin/http-server ./ -c-1 -o",
		"build": "tsc",
		"build:watch": "node dist/dev/watcher"
	},
	"author": "Michael Jonker",
	"license": "MIT",
	"devDependencies": {
		"@types/chokidar": "^2.1.3",
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^18.0.3",
		"browser-sync": "^2.27.10",
		"chokidar": "^3.5.3",
		"fs-extra": "^10.1.0",
		"typedoc": "file:../yaf-scaffold/node_modules/typedoc",
		"typescript": "^4.7.4"
	}
}
