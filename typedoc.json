{
	"$schema": "https://typedoc.org/schema.json",
	"out": "docs",
	"entryPoints": [
		"./src/index.ts",
		"./src/types.d.ts",
		"./src/webComponents/index.ts",
		"./src/webComponents/types.d.ts"
	],
	"externalPattern": ["**/node_modules/**"],
	"excludeExternals": true,
	"excludePrivate": false,
	"excludeProtected": false,
	"excludeNotDocumented": false,
	"media": "./dist/src/assets",
	"logLevel": "Verbose",
	"plugin": [
		"typedoc-theme-yaf",
		"typedoc-plugin-versions",
		"typedoc-plugin-hot-dev"
	],
	"theme": "yaf",
	"hot-dev": {
		"npmScripts": ["build:webComponents:watch"]
	}
}
