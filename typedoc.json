{
	"$schema": "https://typedoc.org/schema.json",
	"out": "docs",
	"entryPoints": [
		"./src/index.ts",
		"./src/shared/types.ts",
		"./src/frontend/index.ts",
		"./src/examples/index.ts"
	],
	"externalPattern": ["**/node_modules/**"],
	"excludeExternals": true,
	"excludePrivate": false,
	"excludeProtected": false,
	"excludeNotDocumented": false,
	"media": "./dist/src/frontend/assets",
	"logLevel": "Verbose",
	"plugin": ["./dist", "typedoc-plugin-versions", "typedoc-plugin-hot-dev"],
	"theme": "yaf",
	"hot-dev": {
		"npmScripts": ["build:frontend:watch", "build:css:watch"]
	}
}
