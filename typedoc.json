{
	"$schema": "https://typedoc.org/schema.json",
	"out": "docs",
	"entryPoints": [
		"./src/backend",
		"./src/frontend",
		"./src/examples",
		"./src/types"
	],
	"externalPattern": ["**/node_modules/**"],
	"excludeExternals": true,
	"excludePrivate": false,
	"excludeProtected": false,
	"excludeNotDocumented": false,
	"media": "./dist/src/frontend/assets",
	"logLevel": "Verbose",
	"plugin": ["typedoc-theme-yaf", "typedoc-plugin-hot-dev"],
	"theme": "yaf",
	"hot-dev": {
		"npmScripts": ["build:frontend:watch", "build:css:watch"]
	}
}
