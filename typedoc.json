{
	"$schema": "https://typedoc.org/schema.json",
	"out": "docs",
	"entryPoints": [
		"./src"
	],
	"externalPattern": [
		"**/node_modules/**"
	],
	"excludeExternals": true,
	"excludePrivate": true,
	"excludeProtected": true,
	"excludeNotDocumented": true,
	"media": "./dist/src/assets",
	"logLevel": "Verbose",
	"plugin": ["./dist"],
	"theme": "yaf",
	"hot-dev": {
		"targetCwd": "../yaf-scaffold",
		"npmScripts": ["build:webComponents:watch"]
	}
}

